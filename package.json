    "@vue/test-utils": "^2.0.0-rc.10",
    "autoprefixer": "^10.3.1",
    "axios": "^0.21.1",
    "babel-loader": "^8.2.2",
    "babel-polyfill": "^6.26.0",
    "browser-sync": "^2.27.4",
    "browser-sync-webpack-plugin": "^2.3.0",
