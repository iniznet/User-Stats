  "description": "A WordPress Vue.js starter plugin",
  "author": "Tareq Hasan <tareq@wedevs.com>",
  "license": "GPLv3",
  "private": true,
  "scripts": {
    "dev": "npm run development",
    "development": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
