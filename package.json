    "@vue/cli-plugin-unit-jest": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/compiler-sfc": "^3.1.4",
    "@vue/test-utils": "^2.0.0-0",
    "autoprefixer": "^10.3.1",
    "axios": "^0.21.1",
    "babel-polyfill": "^6.26.0",
    "browser-sync": "^2.27.4",
